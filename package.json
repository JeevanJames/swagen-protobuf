{
    "name": "swagen-protobuf",
    "version": "1.0.0-beta.3",
    "description": "Swagen generator for Protocol Buffers .proto files",
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts": {
        "lint": "eslint ./**/*.js",
        "bump": "npm version prerelease --no-git-tag-version",
        "prepublishOnly": "npm run lint",
        "postpublish": "npm run bump",
        "update": "npm un --save codewriter && npm i --save codewriter",
        "test": "node ./test-harness"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swagen/swagen-protobuf.git"
    },
    "keywords": [
        "swagen",
        "protobuf",
        "proto"
    ],
    "author": "Swagen <swagen@jeevanjames.com>",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/swagen/swagen-protobuf/issues"
    },
    "homepage": "https://github.com/swagen/swagen-protobuf#readme",
    "dependencies": {
        "codewriter": "^1.3.0",
        "lodash": "^4.17.11"
    },
    "devDependencies": {
        "eslint": "^5.10.0"
    },
    "directories": {
        "lib": "lib"
    }
}
